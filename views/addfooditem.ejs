<html>

<head>
	<link rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.5/css/bootstrap.min.css' />
	<script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
</head>


<body>
	<form id="form01" action='/api/fooditems' method='post'>
		<div class="form-group">
	    <label class="col-sm-2 control-label">Food ID</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" name="foodId">
	    </div>
  	</div>
  	<div class="form-group">
	    <label class="col-sm-2 control-label">Food name</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" name="foodname">
	    </div>
  	</div>
  	<div class="form-group">
	    <label class="col-sm-2 control-label">Company</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" name="companyname">
	    </div>
  	</div>
  	<div class="form-group">
	    <label class="col-sm-2 control-label">Calories</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" name="calories">
	    </div>
  	</div>
  	<div class="form-group">
	    <label class="col-sm-2 control-label">Sodium</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" name="sodium">
	    </div>
  	</div>
  	<div class="form-group">
	    <label class="col-sm-2 control-label">Total Fat</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" name="totalfat">
	    </div>
  	</div>
  	<div class="form-group">
	    <label class="col-sm-2 control-label">Potassium</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" name="potassium">
	    </div>
  	</div>
  	<div class="form-group">
	    <label class="col-sm-2 control-label">Saturated</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" name="saturated">
	    </div>
  	</div>
  	<div class="form-group">
	    <label class="col-sm-2 control-label">Total Carbs</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" name="totalcarbs">
	    </div>
  	</div>
  	<div class="form-group">
	    <label class="col-sm-2 control-label">Polyunsaturated</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" name="polyun">
	    </div>
  	</div>
  	<div class="form-group">
	    <label class="col-sm-2 control-label">Dietary Fiber</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" name="dietfiber">
	    </div>
  	</div>
		<div class="form-group">
	    <label class="col-sm-2 control-label">Monounsaturated</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" name="monoun">
	    </div>
  	</div>
  	<div class="form-group">
	    <label class="col-sm-2 control-label">Sugar</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" name="sugar">
	    </div>
  	</div>
  	<div class="form-group">
	    <label class="col-sm-2 control-label">Trans</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" name="trans">
	    </div>
  	</div>
  	<div class="form-group">
	    <label class="col-sm-2 control-label">Protein</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" name="protein">
	    </div>
  	</div>
  	<div class="form-group">
	    <label class="col-sm-2 control-label">Cholesterol</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" name="cholesterol">
	    </div>
  	</div>
  	<div class="form-group">
	    <label class="col-sm-2 control-label">Vitamin A</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" name="vitamina">
	    </div>
  	</div>
  	<div class="form-group">
	    <label class="col-sm-2 control-label">Vitamin C</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" name="vitaminc">
	    </div>
  	</div>
  	<div class="form-group">
	    <label class="col-sm-2 control-label">Calcium</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" name="calcium">
	    </div>
  	</div>
  	<div class="form-group">
	    <label class="col-sm-2 control-label">Iron</label>
	    <div class="col-sm-10">
	      <input type="text" class="form-control" name="iron">
	    </div>
  	</div>


	  <input id="submitbtn" type="submit" value="Submit">
	</form>

	<div id="result" />
	<script>
		$("#form01").submit(function(event) {
			event.preventDefault();
			var form = $(this),	        
					data = form.serialize(),
					url = form.attr('action');
			$.ajax({
				url: '/api/fooditems',
				data: data,
				type: 'post'
			}).done(function(data) {
				form.trigger('reset');
				alert('Record inserted successfully');
			}).fail(function() {
				alert('Record insertion failed.');
			});
		});
	</script>
</body>
</html>